<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: turquoise;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.SignUp">
   <children>
      <TextField fx:id="name" layoutX="225.0" layoutY="85.0" promptText="name" />
      <TextField fx:id="username" layoutX="226.0" layoutY="130.0" promptText="username" />
      <PasswordField fx:id="password1" layoutX="226.0" layoutY="181.0" promptText="password" />
      <Button fx:id="signUp" layoutX="314.0" layoutY="285.0" mnemonicParsing="false" onAction="#setSignUp" text="sign up">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <PasswordField fx:id="password2" layoutX="226.0" layoutY="226.0" promptText="repassword" />
      <Button fx:id="back" layoutX="226.0" layoutY="284.0" mnemonicParsing="false" onAction="#setBack" prefHeight="26.0" prefWidth="58.0" text="back">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Label fx:id="error" alignment="CENTER" contentDisplay="CENTER" layoutX="-1.0" layoutY="32.0" prefHeight="18.0" prefWidth="600.0" textFill="RED" />
   </children>
</AnchorPane>
