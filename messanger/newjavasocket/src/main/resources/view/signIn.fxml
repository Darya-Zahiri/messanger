<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: turquoise;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.SignIn">
   <children>
      <Label layoutX="178.0" layoutY="80.0" prefHeight="18.0" prefWidth="242.0" text="welcome to messenger">
         <font>
            <Font name="Andalus" size="25.0" />
         </font>
      </Label>
      <TextField fx:id="username" layoutX="225.0" layoutY="143.0" promptText="username" />
      <PasswordField fx:id="password" layoutX="225.0" layoutY="188.0" promptText="password" />
      <Button fx:id="signIn" layoutX="273.0" layoutY="248.0" mnemonicParsing="false" onAction="#setSignIn" opacity="0.32" style="-fx-background-color: blue; -fx-border-color: black;" text="sign in" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="signUp" layoutX="273.0" layoutY="283.0" mnemonicParsing="false" onAction="#setSignUp" text="sign up" />
      <Label fx:id="error" alignment="CENTER" contentDisplay="CENTER" layoutX="-2.0" layoutY="46.0" prefHeight="18.0" prefWidth="607.0" textFill="RED" />
   </children>
</AnchorPane>
